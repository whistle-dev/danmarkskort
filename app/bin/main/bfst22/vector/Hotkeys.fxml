                                       <Label prefHeight="29.0" prefWidth="179.0" text="Start Destination:" textAlignment="CENTER">
                                          <font>
                                             <Font size="20.0" />
                                          </font>
                                          <VBox.margin>
                                             <Insets />
                                          </VBox.margin>
                                       </Label>
                                       <Label prefHeight="30.0" prefWidth="176.0" text="End Destination:" textAlignment="CENTER">
                                          <font>
                                             <Font size="20.0" />
