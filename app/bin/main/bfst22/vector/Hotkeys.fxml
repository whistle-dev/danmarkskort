                              <children>
                                 <VBox alignment="TOP_CENTER" prefHeight="171.0" prefWidth="268.0" spacing="50.0">
                                    <children>
                                       <Label prefHeight="29.0" prefWidth="179.0" text="Create POI:" textAlignment="CENTER">
                                          <font>
                                             <Font size="20.0" />
                                          </font>
                                          <VBox.margin>
                                             <Insets />
                                          </VBox.margin>
                                       </Label>
                                       <Label prefHeight="30.0" prefWidth="176.0" text="Delete POI:" textAlignment="CENTER">
                                          <font>
                                             <Font size="20.0" />
                                          </font>
                                       </Label>
                                    </children>
                                    <HBox.margin>
                                       <Insets left="5.0" top="20.0" />
                                    </HBox.margin>
                                 </VBox>
                                 <VBox alignment="TOP_CENTER" prefHeight="151.0" prefWidth="281.0" spacing="50.0">
                                    <children>
                                       <Label prefHeight="29.0" prefWidth="135.0" text="SHIFT + LEFT CLICK">
                                          <font>
                                             <Font name="Monospaced Regular" size="12.0" />
